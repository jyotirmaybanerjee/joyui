{
  "name": "joyui",
  "version": "0.0.1-SNAPSHOT",
  "description": "A tiny UI components library for React. Heavily inspired by Google's Material design for look and feel.",
  "main": "joyui.js",
  "scripts": {
    "start": "node devServer.js",
    "clean": "rimraf ./dist/",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "build": "webpack --display-error-details --env.dev --config webpack.config.dev.js",
    "build:prod": "webpack --env.prod -p --config webpack.config.prod.js",
    "lint": "eslint ./src webpack.config.*.js && echo 'No Lint errors found.' "
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/jyotirmaybanerjee/joyui.git"
  },
  "keywords": [
    "React",
    "UI",
    "Components",
    "Material",
    "Design",
    "ES6",
    "ES7"
  ],
  "author": "Jyotirmay Banerjee",
  "license": "BSD",
  "homepage": "https://bitbucket.org/jyotirmaybanerjee/joyui#readme",
  "dependencies": {
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-literals": "^6.8.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.8.0",
    "rimraf": "^2.5.4",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.0",
    "scss-loader": "0.0.1",
    "style-loader": "^0.13.1",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-server": "2.0.0-beta",
    "webpack-hot-middleware": "^2.12.2"
  }
}
